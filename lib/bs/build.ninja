ninja_required_version = 1.5.1 
bs_package_flags = -bs-package-name bs-qs
src_root_dir = /Users/ryan/Personal/proj/bs-qs
bsc = /Users/ryan/Personal/proj/bs-qs/node_modules/bs-platform/bin/bsc.exe
bsdep = /Users/ryan/Personal/proj/bs-qs/node_modules/bs-platform/bin/bsb_helper.exe
ocamllex = ocamllex.opt
bsc_flags = -nostdlib -I /Users/ryan/Personal/proj/bs-qs/node_modules/bs-platform/lib/ocaml -w -10 -no-alias-deps -color always -w -40+6+7+27+32..39+44+45
ppx_flags = 
bs_package_includes = 
bs_package_dev_includes = 
refmt = /Users/ryan/Personal/proj/bs-qs/node_modules/bs-platform/bin/refmt.exe
reason_react_jsx = 
refmt_flags = --print binary
bsb_dir_group = 0
bsc_lib_includes = -I src
bsc_group_1_includes = -I test
rule build_ast_and_deps_from_reason_impl
  command = ${bsc} -pp "${refmt} ${refmt_flags}" ${reason_react_jsx}  ${ppx_flags} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast -impl ${in}
  description = [34mBuilding[39m [2m${out}[22m
build test/index.mlast : build_ast_and_deps_from_reason_impl $src_root_dir/test/index.re
rule build_deps
  command = ${bsdep} -g ${bsb_dir_group} -MD ${in}
  description = [34mBuilding[39m [2m${out}[22m
build test/index.mlast.d : build_deps test/index.mlast
  bsb_dir_group = 1
rule build_cmj_cmi
  command = ${bsc} ${bs_package_flags} -bs-assume-no-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${bsc_flags} -o ${in} -c  ${in} ${postbuild}
  depfile = ${in}.d
  description = [34mBuilding[39m [2m${out}[22m
build test/index.cmj $src_root_dir/lib/js/test/index.js test/index.cmi : build_cmj_cmi test/index.mlast
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/test
  bs_package_includes = $bs_package_includes $bs_package_dev_includes
  bsc_extra_includes = ${bsc_group_1_includes}
build src/qs.mlast : build_ast_and_deps_from_reason_impl $src_root_dir/src/qs.re
build src/qs.mlast.d : build_deps src/qs.mlast
build src/qs.cmj $src_root_dir/lib/js/src/qs.js src/qs.cmi : build_cmj_cmi src/qs.mlast
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/src
build build.ninja : phony  ||  src/qs.mlast.d test/index.mlast.d
